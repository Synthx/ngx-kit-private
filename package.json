{
    "name": "kit-namespace",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "build": "npm run build:styles && ng build kit --configuration development",
        "build:styles": "sass --style=compressed projects/kit/styles/index.scss projects/kit/assets/styles.css",
        "build:prod": "npm run build:styles && ng build kit",
        "build:storybook": "npm run build:styles && ng run kit:build-storybook",
        "publish": "npm run build:prod && cd dist/kit && npm publish",
        "start": "ng run kit:storybook",
        "format": "npx prettier --write .",
        "format:check": "npx prettier --check .",
        "lint": "ng lint"
    },
    "dependencies": {
        "@angular/animations": "^15.1.0",
        "@angular/common": "^15.1.0",
        "@angular/compiler": "^15.1.0",
        "@angular/core": "^15.1.0",
        "@angular/forms": "^15.1.0",
        "@angular/platform-browser": "^15.1.0",
        "@angular/platform-browser-dynamic": "^15.1.0",
        "@angular/router": "^15.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.1.5",
        "@angular-eslint/builder": "15.2.1",
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular-eslint/eslint-plugin-template": "15.2.1",
        "@angular-eslint/schematics": "15.2.1",
        "@angular-eslint/template-parser": "15.2.1",
        "@angular/cli": "~15.1.2",
        "@angular/compiler-cli": "^15.1.0",
        "@babel/core": "^7.20.12",
        "@compodoc/compodoc": "^1.1.19",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-interactions": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/angular": "^6.5.16",
        "@storybook/builder-webpack5": "^6.5.16",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/testing-library": "^0.0.13",
        "@typescript-eslint/eslint-plugin": "5.48.2",
        "@typescript-eslint/parser": "5.48.2",
        "babel-loader": "^8.3.0",
        "eslint": "^8.33.0",
        "ng-packagr": "^15.1.0",
        "prettier": "2.8.4",
        "typescript": "~4.9.4"
    }
}
